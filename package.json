    "babel-preset-stage-0": "^6.5.0",
    "koa": "^1.2.1",
    "koa-body": "^1.4.0",
    "koa-cors": "0.0.16",
    "koa-logger": "^1.3.0",
    "koa-router": "^5.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-polyfill": "^6.13.0",
