{
  "appenders": [
    {
      "type": "console"
    },
    {
      "type": "file",
      "filename": "log/static_server.log",
      "maxLogSize": 2048,
      "pattern": ".yyyy-MM-dd",
      "backups": 3,
      "category": "Detail"
    }
  ],
  "levels": {
    "[all]": "INFO",
    "Detail": "TRACE"
  }
}